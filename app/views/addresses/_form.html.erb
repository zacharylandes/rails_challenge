      <%=form_for [@student,@address] do |f| %>
      <%=f.label :description%>
      <%=f.text_field :description%>

        <%= f.label :street  %>
        <%= f.text_field :street  %>
        <%= f.label :city  %>
        <%= f.text_field :city  %>
           <%= f.label :state  %>
                <%= f.text_field :state  %>
                      <%= f.label :zipcode  %>
                        <%= f.number_field :zipcode  %>
        <%= f.submit %>
      <% end %>
